<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lionortiger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tigerlionappicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tigerlionappicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Support"></activity>
        <activity
            android:name=".Menu_about"
            android:label="@string/title_activity_menu_about"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Rules" />
        <activity android:name=".Front_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>